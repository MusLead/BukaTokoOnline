<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.lazday.bukatokoonline">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/lzd_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/lzd_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".activity.TransActivity"></activity>
        <activity
            android:name=".activity.CityActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.OngkirActivity" />
        <activity android:name=".activity.CartActivity" />
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.ProfileActivity"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.SignupActivity" />
        <activity android:name=".activity.DetailActivity" />
        <activity
            android:name=".activity.SplashScreen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>